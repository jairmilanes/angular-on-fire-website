{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "dom",
      "es6"
    ],
    "types": [
      "node"
    ],
    "experimentalDecorators": true
  },
  "include": [
    "integration/**/*.ts",
    "support/**/*.ts"
  ],
  "exclude": [
    "../node_modules"
  ],
  "compileOnSave": false
}
